{
  "functions": {
    "server/main.py": {
      "runtime": "python3.9"
    }
  },
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": { 
        "outputDirectory": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "client/build/$1"
    }
  ]
}